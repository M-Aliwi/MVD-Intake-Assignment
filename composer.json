{
    "name": "mvd/intake-assignment",
    "description": "MVD Intake Assignment - Bid submission system",
    "type": "project",
    "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
    "require": {
        "symfony/flex": "*",
        "symfony/framework-bundle": "*",
        "symfony/console": "*",
        "symfony/twig-bundle": "*",
        "symfony/maker-bundle": "*",
        "symfony/http-client": "*",
        "symfony/messenger": "*",
        "symfony/validator": "*",
        "symfony/routing": "*",
        "symfony/runtime": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.18",
        "doctrine/doctrine-migrations-bundle": "^3.5",
        "doctrine/orm": "^3.5",
        "symfony/yaml": "^7.3",
        "symfony/dotenv": "^7.3",
        "symfony/form": "^7.3"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
